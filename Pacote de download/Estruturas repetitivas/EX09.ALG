Algoritmo "EX09"


var

   entrada , resultado, cont: inteiro

Inicio

   escreva("Deseja a tabuada para qual valor?")
   leia(entrada)
   cont <-1
   enquanto cont <= 10 faca
     resultado <- entrada * cont
     escreval(entrada," x ",cont," = ",resultado)
     cont <- cont +1
   fimenquanto
   





Fimalgoritmo
