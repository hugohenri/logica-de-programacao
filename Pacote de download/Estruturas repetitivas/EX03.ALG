Algoritmo "EX03"


var
  senha, digitada :inteiro



Inicio

    senha <- 2002
    
    escreval("Digite a senha: ")
    leia(digitada)
    
    enquanto digitada <> senha faca
      Escreva("Senha Invalida! Tente novamente: ")
      leia(digitada)
    fimenquanto
    
    escreval("Acesso permitido!")





Fimalgoritmo
