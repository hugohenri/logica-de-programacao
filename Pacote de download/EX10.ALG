Algoritmo "EX10"





Var

   distancia, combustivel, consumo : real


Inicio

    escreva("Distancia pecorrida: ")
    leia(distancia)
    escreva("Combustivel gasto: ")
    leia(combustivel)

    
   consumo <- distancia / combustivel




   
   
   escreva("Consumo medio:",consumo:8:3)




Fimalgoritmo